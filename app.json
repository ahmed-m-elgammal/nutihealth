{
    "expo": {
        "name": "NutriHealth",
        "slug": "nutrihealth",
        "version": "1.0.0",
        "sdkVersion": "54.0.0",
        "orientation": "portrait",
        "scheme": "nutrihealth",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": false,
            "bundleIdentifier": "com.nutrihealth.app",
            "infoPlist": {
                "NSCameraUsageDescription": "NutriHealth needs access to your camera to scan barcodes and take food photos.",
                "NSPhotoLibraryUsageDescription": "NutriHealth needs access to your photo library to save and load meal photos.",
                "NSMotionUsageDescription": "NutriHealth uses motion data to better estimate your calorie burn during activities."
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.nutrihealth.app",
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE",
                "ACCESS_FINE_LOCATION",
                "ACTIVITY_RECOGNITION"
            ]
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        },
        "plugins": [
            "expo-router",
            [
                "expo-camera",
                {
                    "cameraPermission": "Allow NutriHealth to access your camera to scan barcodes and take food photos."
                }
            ],
            [
                "expo-media-library",
                {
                    "photosPermission": "Allow NutriHealth to access your photo library.",
                    "savePhotosPermission": "Allow NutriHealth to save photos to your library."
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification-icon.png",
                    "color": "#22c55e"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "nutrihealth-app-id"
            }
        }
    }
}